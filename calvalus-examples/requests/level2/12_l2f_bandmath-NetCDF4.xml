<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<wps:Execute service="WPS"
             version="1.0.0"
             xmlns:wps="http://www.opengis.net/wps/1.0.0"
             xmlns:ows="http://www.opengis.net/ows/1.1"
             xmlns:xlink="http://www.w3.org/1999/xlink">

    <ows:Identifier>L2F</ows:Identifier>

    <wps:DataInputs>
        <wps:Input>
            <ows:Identifier>productionName</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>example level2 02_l2f_bandmath-NetCDF4</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>inputPath</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/projects/example/level2/00_l2_bandmath/L2_of_MER_..._1.*${yyyy}${MM}${dd}.*.seq$</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>calvalus.output.dir</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/projects/example/level2/02_l2f_bandmath-NetCDF4</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>minDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-03-03</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>maxDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-03-04</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>calvalus.output.format</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>NetCDF4-BEAM</wps:LiteralData>
            </wps:Data>
        </wps:Input>

    </wps:DataInputs>
</wps:Execute>